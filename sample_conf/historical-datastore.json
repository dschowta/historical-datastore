{
  "http": {
    "publicEndpoint": "http://localhost:8085",
    "bindAddr": "0.0.0.0",
    "bindPort": 8085
  },
  "web": {
    "bindAddr": "0.0.0.0",
    "bindPort": 4000,
    "staticDir": "../web"
  },
  "registry": {
    "backend": {
      "type": "leveldb",
      "dsn": "registry.ldb"
    }
  },
  "data": {
    "backend": {
      "type": "mongodb",
      "dsn": "mongodb://hds:admin@ucc-docker:27017/hdsdb"
    },
    "autoRegistration": false
  },
  "aggregation": {},
  "serviceCatalogs": [
    {
      "discover": false,
      "endpoint": "http://localhost:8082/sc",
      "ttl": 60
    }
  ]
}
