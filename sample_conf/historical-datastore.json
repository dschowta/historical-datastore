{
  "serviceID": "",
  "http": {
    "publicEndpoint": "http://public-endpoint",
    "bindAddr": "0.0.0.0",
    "bindPort": 8085
  },
  "web": {
    "bindAddr": "0.0.0.0",
    "bindPort": 4000,
    "staticDir": "../web"
  },
  "registry": {
    "backend": {
      "type": "leveldb",
      "dsn": "./hds-registry"
    }
  },
  "data": {
    "backend": {
      "type": "influxdb",
      "dsn": "http://user:password@influxdb-hostname:8086/hds"
    },
    "retentionPeriods": ["1h", "1w"],
    "autoRegistration": false
  },
  "serviceCatalogs": [],
  "auth": {}
}
